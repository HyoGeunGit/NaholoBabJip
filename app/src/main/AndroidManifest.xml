<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shimhg02.solorestorant">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:usesCleartextTraffic="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.shimhg02.solorestorant.ui.Activity.LogIn.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.shimhg02.solorestorant.ui.Activity.Main.MainActivity"/>
        <activity android:name="com.shimhg02.solorestorant.ui.Activity.SignUp.SignUpPhoneActivity"/>
        <activity android:name="com.shimhg02.solorestorant.ui.Activity.SignUp.SignUpInfoActivity"/>
        <activity android:name="com.shimhg02.solorestorant.ui.Activity.SignUp.SignUpIdActivity"/>
        <activity android:name="com.shimhg02.solorestorant.ui.Activity.SignUp.SignUpProfileActivity"/>
        <activity android:name="com.shimhg02.solorestorant.ui.Activity.Term.TermActivity"/>
        <activity android:name=".ui.Activity.Story.StoryTest"/>
    </application>

</manifest>